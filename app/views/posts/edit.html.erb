<h1>글쓰기</h1>
<%= form_for @post, url: "/posts/update/#{@post.id}" do |f| %>
    <%= f.text_field :title, style: "width: 400px; height: 50px" %><br><br>
    <%= f.text_area :memo, style: "width: 400px; height: 100px" %><br>
    <%= f.file_field :image, value: @post.img_url%>
    <%= f.submit '작성' %>
<% end %>